::StoryCSS [stylesheet]
/* anton-regular - latin */
@font-face {
  font-family: 'Anton';
  font-style: normal;
  font-weight: 400;
  src: local('Anton Regular'), local('Anton-Regular'),
       url('fonts/anton-v8-latin-regular.woff2') format('woff2'), /* Chrome 26+, Opera 23+, Firefox 39+ */
       url('fonts/anton-v8-latin-regular.woff') format('woff'); /* Chrome 6+, Firefox 3.6+, IE 9+, Safari 5.1+ */
}

tw-passage{
	font-family: 'Anton', sans-serif;
}

tw-passage .lang-select{
	text-align: center;
}

tw-link{
	color: #ff0000;
}

tw-link:hover{
	color: #B21212;
}

tw-enchantment.link{
	color: #FF7A19;
}

tw-enchantment.link:hover{
	color: #FF3100;
}

tw-sidebar tw-icon.redo {
	display: none;
}
tw-sidebar tw-icon.undo {
	display: none;
}
